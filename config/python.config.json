            "stargazers": {
                "include": false
            },
            "subscribers": {
                "include": false
            },
            "events": {
                "include": false,
                "repositories": {
                    "include": false
                }
            },
            "owner": {
                "include": true
            },
            "template": {
                "include": true
            },
            "parent": {
                "include": true
            },
            "source": {
