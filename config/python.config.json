{
    "output": {
        "directory": [".", "generated"],
        "index_name": "index.json",
        "include_config": {
            "navigation": "navigation.json"
        }
    },
    "github": {
        "repository_api": "https://api.github.com/repos/LittleTealeaf/littletealeaf.github.io",
        "path": ["github"],
        "api": {
            "timeout_delay": 300,
            "keys": {
                "api_status": "fetched_api"
            }
        },
        "repositories": {
            "true": false,
            "remove_private": true,
            "force_api": false,
            "count": 1000,
            "path": ["github", "repositories"],
            "remove_keys": [
                "permissions", "allow_squash_merge", "allow_merge_commit", "allow_rebase_merge", "allow_auto_merge", "delete_branch_on_merge", "temp_clone_token", "allow_update_branch"
            ],
            "contributors": {
                "include": false,
                "include_bots": false
            },
            "stargazers": {
                "include": false
            },
            "subscribers": {
                "include": false
            },
            "events": {
                "include": false,
                "repositories": {
                    "include": false
                }
            },
            "owner": {
                "include": true,
                "repositories": {
                    "include": false
                }
            },
            "template": {
                "include": true
            },
            "parent": {
                "include": true
            },
            "source": {
                "include": true
            },
            "organization": {
                "include": true
            },
            "branches": {
                "include": true,
                "url_remove": ["{/branch}"]
            },
            "tags": {
                "include": true
            },
            "languages": {
                "include": true
            },
            "commits": {
                "include": false,
                "url_remove": ["{/sha}"]
            },
            "contents": {
                "include": false,
                "url_remove": ["{+path}"]
            },
            "issues": {
                "include": false,
                "url_remove": ["{/number}"],
                "repositories": {
                    "include": false,
                    "force_ref": true
                }
            },
            "forks": {
                "include": false,
                "count": 50
            }
        },
        "users": {
            "count": 20,
            "force_api": false,
            "path": ["github", "users"],
            "followers": {
                "include": false
            },
            "following": {
                "include": false,
                "url_remove": ["{/other_user}"]
            },
            "gists": {
                "include": false,
                "url_remove": ["{/gist_id}"]
            },
            "events": {
                "include": false,
                "url_remove": ["{/privacy}"],
                "url_add": "/public",
                "users": {
                    "include": true,
                    "force_ref": true
                }
            },
            "organizations": {
                "include": false
            },
            "starred": {
                "include": false,
                "url_remove": ["{/owner}", "{/repo}"]
            },
            "avatar": {
                "include": true,
                "circular": true
            },
            "repositories": {
                "include": false,
                "count": 1000,
                "owner": {
                    "force_ref": true,
                    "include": false
                }
            },
            "remove_keys": [
                "private_gists", "total_private_repos", "owned_private_repos", "disk_usage", "collaborators",
                "two_factor_authentication", "plan", "contributions"
            ]
        },
        "events": {
            "count": 100,
            "force_api": false,
            "path": ["github", "events"],
            "repo": {
                "include": false,
                "force_api": true
            },
            "payload": {
                "commits": {
                    "include": false,
                    "force_api": true
                },
                "pull_request": {
                    "include": false
                },
                "issue": {
                    "include": false
                }
            }
        },
        "gists": {
            "force_api": false,
            "count": 100,
            "path": ["github", "gists"],
            "owner": {
                "include": true
            }
        },
        "organizations": {
            "count": 20,
            "force_api": false,
            "path": ["github", "organizations"],
            "avatar": {
                "include": true,
                "circular": true
            },
            "public_members": {
                "include": true
            },
            "repositories": {
                "include": true
            },
            "events": {
                "include": false
            },
            "remove_keys": [
                "email", "private_gists", "total_private_repos", "owned_private_repos", "disk_usage", "collaborators",
                "two_factor_authentication", "plan", "contributions"
            ]
        },
        "commits": {
            "force_api": false,
            "count": 20,
            "path": ["github", "commits"],
            "author": {
                "include": true
            },
            "committer": {
                "include": true
            }
        },
        "contents": {
            "force_api": false,
            "path": ["github", "contents"],
            "count": 100
        },
        "issues": {
            "count": 20,
            "force_api": false,
            "path": ["github", "issues"],
            "user": {
                "include": true
            },
            "assignees": {
                "include": true
            },
            "comments": {
                "include": true
            },
            "repositories": {
                "include": true
            },
            "events": {
                "include": false
            }
        },
        "pull_requests": {
            "count": 20,
            "force_api": false,
            "example": "https://api.github.com/repos/LittleTealeaf/littletealeaf.github.io/pulls/419"
        },
        "releases": {
            "force_api": false,
            "count": 10,
            "path": ["github", "releases"],
            "author": {
                "include": true
            }
        },
        "tags": {
            "force_api": false,
            "count": 10,
            "path": ["github", "tags"],
            "commit": {
                "include": true
            }
        },
        "languages": {
            "force_api": false,
            "path": ["github", "languages"]
        },
        "branches": {
            "force_api": false,
            "count": 50,
            "path": ["github","branches"],
            "commit": {
                "include": true
            }
        }
    },
    "images": {
        "path": ["images"],
        "circular": false,
        "width": null,
        "height": null
    },
    "config": {
        "projects": {
            "path": "./config/projects.json"
        },
        "resume": {
            "path": "./config/resume.json"
        }
    }
}