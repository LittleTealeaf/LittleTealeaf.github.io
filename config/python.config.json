                "include": true
            }
        },
        "users": {
            "count": 20,
            "followers": {

            },
            "following": {

            },
            "events": {

            },
            "starred": {

            },
            "remove_keys": [
                "email", "private_gists", "total_private_repos", "owned_private_repos", "disk_usage", "collaborators",
                "two_factor_authentication", "plan","contributions"
            ]
        },
        "events": {
            "count": 100,
